{
  "compilerOptions": {
    "target": "ESNext",
    "module": "Node16",
    "jsx": "react-native",
    "moduleResolution": "node16",
    "allowJs": true,
    "strict": true,
    "baseUrl": ".",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
